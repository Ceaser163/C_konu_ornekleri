#include <stdio.h>
#include <stdlib.h>

int main()
{
//Kullanıcıdan iki sayı alarak hangisinin büyük ya da eşit mi olduğunu bulma.

   int sayi1,sayi2;

   printf("Lutfen 2 adet sayi giriniz\n");
   scanf("%d%d",&sayi1,&sayi2);

   if(sayi1 == sayi2)
   {
       printf("Sonuc : %d = %d",sayi1,sayi2);
   }
   else if(sayi1 > sayi2)
   {
        printf("Sonuc : %d > %d",sayi1,sayi2);
   }
   else
   {
      printf("Sonuc : %d < %d",sayi1,sayi2);
   }


    return 0;
}
