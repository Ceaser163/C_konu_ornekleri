//Üçgenin Eşitkenar İkizkenar Çeşitkenar Olduğunu Belirleyen Program

#include <stdio.h>
#include <stdlib.h>

int main()
{
    int a,b,c;
    
    printf("1. acıyı giriniz\n");
    scanf("%d",&a);
    printf("2. acıyı giriniz\n");
    scanf("%d",&b);
    printf("3. acıyı giriniz\n");
    scanf("%d",&c);
    
    if(a+b+c==180){ 
    	
    	if(a==b && b==c){
    		printf("Girdiginiz acilar Eskenar ucgen olusturur.");
    }
    	else if (a==b || b==c || a==c){
    		printf("Girdiginiz acilar ikizkenar ucgen olusturur.");
		}
	    else {
	    	printf("Girdiginiz acilar cesitkenar ucgen olusturur.");
		}
    }
		else {
			printf("Girdiginiz acilar ile ucgen olusmaz.");
		}
	
    return 0;
}
